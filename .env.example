# Qwen Code On-Premise Configuration
# Copy this file to .env and update the values

# Internal LLM Server Configuration
OPENAI_BASE_URL=http://your-internal-llm:8080/v1
OPENAI_API_KEY=internal-api-key
OPENAI_MODEL=qwen3-235b-a22b-2507

# On-Premise Mode
ON_PREMISE_MODE=true

# Optional: SSL Certificate Bypass (for internal servers without valid certificates)
NODE_TLS_REJECT_UNAUTHORIZED=0

# Optional: Internal Web Search Configuration
INTERNAL_WEB_CONFIG_PATH=./internal-web-config.json

# Optional: Debug Mode
DEBUG=false

# Optional: Custom Model Parameters
# TEMPERATURE=0.7
# MAX_TOKENS=2048